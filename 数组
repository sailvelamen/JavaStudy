public class Main {
	public static void main(String[] args) {
		
		int[] nums = new int[5];
			//声明整形数组变量叫nums在栈内存当中，在堆内存里分配了5个整型(int)空间指向nums(一个地址)
			//数组名就是新分配内存空间的首地址
		System.out.println(nums);//打印的是地址
    
    /*		//int year[6] = {1,2,3,4,5,6,7};
					//错误：声明并初始化数组是不需要指定数组的大小
				
				int days[] = {1,15};	//对的
				
				int array[] = {};	//语法正确：但不能为数组元素赋值
		*/
	}
}
